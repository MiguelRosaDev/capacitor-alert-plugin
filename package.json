{
  "name": "capacitor-alert-plugin",
  "version": "1.0.0",
  "description": "A simple Capacitor plugin to show system alerts",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^5.0.0"
  },
  "devDependencies": {
    "@capacitor/android": "^5.0.0",
